#template("xwikivars.vm")
#template("layoutvars.vm")
#template("htmlheader.vm")
#if($displayMainMenu)
    #template("menuview.vm")
#end
#if($viewer && $viewer!="view" && $viewer!="content")
    <!-- dedicated viewer: $viewer -->
#else
    #template("header.vm")
    <div id="wrapper-content">
    <div class="container" id="the-content">
        #if($request.bc || $doc.getObject("CurrikiCode.AssetClass") && $doc.parent || $doc.getObject("CurrikiCode.CompositeAssetClass"))
        <div class="row" id="the-breadcrumb">
            <div class="col-sm-6 col-xs-12">
                #ru_breadcrumb()
                #ru_getTOC()
                #if("$!parentPage"!="")
                <div id="dropable-toc">
                </div>
                #end
            </div>
            <div class="col-sm-6 col-xs-12 text-right">
                <div class="btn-group" id="the-pagination">
            #if("$!parentPage"!="")
                #computePrevAndNext($parentPage)
                #set($bc="$!request.bc")##
                #if("$!prev"!="")
                    <a href="$xwiki.getURL($prev)#if($bc!="")?bc=$bc#end" ##
                       class="btn btn-custom-nav" id="nav-prev-page"><i class="icon-chevron-left icon-large"></i> <!-- Previous--></a>
                #else
                    <a href="#" class="btn btn-custom-nav-disabled" id="nav-prev-page"><i class="icon-chevron-left icon-large"></i> <!-- Previous--></a>
                #end
                #if("$!next"!="")
                    <a href="$xwiki.getURL($next)#if($bc!="")?bc=$bc#end" ##
                       class="btn btn-custom-nav" id="nav-next-page"><!-- Next--> <i class="icon-chevron-right icon-large"></i></a>
                #else
                    <a href="#" class="btn btn-custom-nav-disabled" id="nav-next-page"><!-- Next--> <i class="icon-chevron-right icon-large"></i></a>
                #end
                </div>
            #end
            </div>
        </div>
        #end
    <div class="row">
    <div class="col-md-12" id="main-content">
#end
